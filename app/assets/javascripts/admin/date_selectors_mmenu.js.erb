function DateSelectorMMenu() {

  $("input.datepicker").each(function(i) {
    return $(this).datepicker({
      beforeShow:function(textbox, instance){
        $('.mmenu-panel').append($('#ui-datepicker-div'));
        $('#ui-datepicker-div').hide();
      }
    });
  });
};

$(document).on('page:load', DateSelectorMMenu);
$(document).ready(DateSelectorMMenu);