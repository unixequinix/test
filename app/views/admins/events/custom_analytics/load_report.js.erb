<% if @data.eql?("[]") %>
  $("#analytics-text").hide();
  $("#load-report .content").html("<%= escape_javascript(render partial: 'no_report', locals: { name: @name }) %>");
<% else %>
  $("#load-report .content").html("<%= escape_javascript(render partial: 'report', locals: { name: @name, cols: String(@cols), rows: String(@rows), metric: String(@metric), decimals: @decimals, aggregator: @aggregator, data: @data, action: @action}) %>");
<% end %>
